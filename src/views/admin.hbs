<div class='container'>
    <!-- Header Section -->
    <div class='header'>
        <h1>Admin Panel</h1>
        <p>Manage incoming messages and submissions from users.</p>
    </div>

    <!-- Messages Section -->
    <section class='section'>
        <h2>Messages</h2>
        <p>Here are the latest messages submitted through the contact form:</p>

        <!-- Message List -->
        <div class='message-list'>
            <div class='message-item'>
                <h3>John Doe</h3>
                <p>Email: john.doe@example.com</p>
                <p>Message: "Hi, I would like to know more about your services."</p>
                <button class='btn btn-delete'>Delete</button>
            </div>

            <div class='message-item'>
                <h3>Jane Smith</h3>
                <p>Email: jane.smith@example.com</p>
                <p>Message: "Can I book a consultation with you?"</p>
                <button class='btn btn-delete'>Delete</button>
            </div>

            <!-- Additional messages can be dynamically rendered here -->
        </div>
    </section>

</div>